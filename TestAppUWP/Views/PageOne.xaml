<Page
    x:Class="TestAppUWP.Views.PageOne"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="White">
        <StackPanel>
            <TextBlock Text="Text From view model"/>
            <TextBlock FontSize="24" Text="{Binding VmText}"/>
            <Button Margin="10,10,10,10" Background="LightGray" Content="Go to subpage 1"  Command="{Binding GoToSubPageOneOneCommand, Mode=TwoWay}" />
        </StackPanel>

    </Grid>
</Page>
